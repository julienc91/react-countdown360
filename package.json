{
  "name": "react-countdown360",
  "version": "1.0.0",
  "description": "A simple and customizable React circular countdown",
  "homepage": "https://github.com/julienc91/react-countdown360",
  "repository": "https://github.com/julienc91/react-countdown360",
  "author": "Julien Chaumont <github@julienc.io>",
  "license": "MIT",
  "keywords": [
    "react",
    "react-component",
    "countdown"
  ],
  "main": "lib/index.js",
  "module": "lib/esm.index.js",
  "directories": {
    "lib": "lib/"
  },
  "scripts": {
    "build": "rm -rf lib && npm run build:cjs && npm run build:esm",
    "build:cjs": "babel src --out-dir lib",
    "build:esm": "cross-env BABEL_ENV=esm babel src --out-dir lib/esm",
    "prepare": "npm run build",
    "lint": "eslint ./"
  },
  "peerDependencies": {
    "react": ">=16.3"
  },
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-react": "^7.15.1",
    "eslint-plugin-standard": "^4.0.0",
    "react": "^16.10.1",
    "react-dom": "^16.10.1"
  }
}
